{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "strict": true,
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@ui/*": ["packages/ui/src/*"],
      "@gigmatch/db": ["packages/db/src/index.ts"],
      "@gigmatch/db/*": ["packages/db/src/*"],
      "@utils/*": ["packages/utils/src/*"],
      "@ai/*": ["packages/ai-models/src/*"]
    }
  }
}
